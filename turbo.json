{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "//#build": {
      "outputs": ["dist/**"]
    },
    "logsdx-site#build": {
      "dependsOn": ["//#build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "logsdx-site#dev": {
      "dependsOn": ["//#build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
