{
  "name": "logsdx",
  "version": "0.1.1",
  "private": false,
  "packageManager": "bun@1.2.9",
  "workspaces": [
    "site"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "bun run build:cjs && bun run build:esm && bun run build:types && bun run generate-schema",
    "build:cjs": "bun build src/index.ts --outfile dist/index.cjs --format cjs --minify",
    "build:esm": "bun build src/index.ts --outfile dist/index.mjs --format esm --minify",
    "build:types": "tsc --noEmit --skipLibCheck --emitDeclarationOnly --declaration --outDir dist",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "bun run prettier --write .",
    "format:check": "bun run prettier --check .",
    "check": "bun run lint && bun run format:check && bun run test",
    "generate-css": "bun run scripts/generate-css.ts",
    "generate-schema": "bun run scripts/generate-schema.ts",
    "demo": "bun run demo-dracula-consistency.js",
    "turbo:build": "turbo run build",
    "turbo:dev": "turbo run dev",
    "site:dev": "cd site && bun run dev",
    "site:build": "cd site && bun run build",
    "site:start": "cd site && bun run start",
    "dev": "cd site && bun run dev"
  },
  "homepage": "https://github.com/yowainwright/logsdx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/logsdx.git"
  },
  "bugs": {
    "url": "https://github.com/yowainwright/logsdx/issues"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/bun": "latest",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "bun-types": "^1.2.2",
    "commander": "^12.1.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "semantic-release": "^24.1.2",
    "ts-jest": "^29.2.5",
    "ts-morph": "^25.0.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "ansi-styles": "^6.2.1",
    "chalk": "^5.4.1",
    "colors": "^1.4.0"
  }
}